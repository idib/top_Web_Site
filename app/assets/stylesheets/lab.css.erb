div.like{
	box-shadow:  3px 3px 10px #00c;
	float: left;
	background: url(<%= asset_path("like.png") %>) no-repeat center center ;
	height: 98px;
	width:  82px;
	-webkit-background-size: contain;
	-moz-background-size: contain;
	-o-background-size: contain;
	background-size: contain;
}
.pressed{
	box-shadow: inset 3px 3px 10px #00c !important;
}

#table-block *{
	font-size: 14pt;
	font-family: Times New Roman;
}

