@font-face {
    font-family: Azerbaycan; 
    src: url(<%= asset_path("8LMprz3N.otf") %>);
}

*{	
	font-family: Azerbaycan;
	padding:0px;
	margin:0px;
}

.header{
	width:100%;
}

.log{
	z-index:99;
	transition: all 0.5s ease;
	position:absolute;
}

.in{
transition: all 0.5s ease;
top:25px;
right:25px;
text-align:center;
background: #FF0033;
border:0px;
overflow:hidden;

}

.in>span{
-webkit-user-select:none;
color:white;
text-align:center;
font-size:32px;
background: #FF0033;
cursor: pointer;
}

.panel{
opacity:0;
transition: all 0.5s ease;
overflow:hidden;
padding:0px;
color:white;
text-align:center;
width:175px;
font-size:18px;
background: #FF0033;
height:0;
}

.panel>form{
transition: all 0.5s ease;
height:0;
}

.panel.active>form{
height:100%;
}

.panel.active{
transition: all 0.5s ease;
padding:25px;
height:100%;
opacity:1;
}

input[type="text"],
input[type="password"],
input[type="submit"]{
	padding:5px;
	margin:15px;
	width:80%;
	color:black;
}

input[type="submit"]{
	background:white;
	border:0px;
	cursor: pointer;
}

.manager{
	z-index:99;
	position:fixed;
	top:25px;
	width:50%;
	left:25%;
	background:rgba(192,192,192,.9);
	border-radius:15px;
}

.manager>div.url{
	text-align:center;
	padding:5px;
}

.menu {
	z-index:99;
	position:fixed;
	left:25px;
	top: 0;
}

.menu>ul{
	list-style-type:none;
	-webkit-margin-before: 0em;
	-webkit-margin-after: 0em;
	-webkit-margin-start: 0px;
	-webkit-margin-end: 0px;
	-webkit-padding-start: 0px;
}

li.selected{
	z-index:99;
	text-align:right;
	transition: all 0.5s ease;
	position:absolute;
	left: -25px;
	cursor:pointer;
}



li.selected>span>a{
	transition: all 0.5s ease;
	position:absolute;
	color:white;
	font-size:25px;
	padding:7px;
	height: 50px;
	width:150px;
	background:#6699FF;
	border-right:#0000CC solid 5px;
	text-decoration: none;
}

li.selected>span>a:hover{
	transition: all 0.5s ease;
	width: 155px;
	border-right:#00CC00 solid 5px;
}

.sub{
	position: static;
}

.sub>span{
	transition: all 0.5s ease;
	position:absolute;
	color:black;
	font-size:25px;
	padding:7px;
	height: 25px;
	width:75px;
	background:#3300CC;
}

.lab{
	margin-left: 200px;
	margin-top: 170px;
	max-width:calc(100% - 250px);
	border:1px gray solid;
	border-radius:30px;
}

.lab>span{
	display:block;
	margin:30px;
	font-size:19px;
	
}

.rab{
	margin:30px;
	width:300px;
	display:inline-block;
}

.rab>img{
	width:100%;
}


.rabota{
	width:80%;
	margin-left:250px;
	margin-top:150px;
}

.img{
	width:50%;
	float:left;
}

.img>img{
	width:150px;
}

.rabota>.text{
	width:50%;
	float:left;
	background:gray;
	border-radius:40px;
}

.rabota>.text>span{
	display:block;
	background:white;
	margin:30px;
	border-radius:30px;
	padding:15px;
}

.rabota>.text>div{
	margin:30px;
}

