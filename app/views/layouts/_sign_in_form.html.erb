<div id="logIn" class="log in" >
	<span onclick="formIn()">Вход</span>
	<div id = "logPanel" class="panel">
		<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
		<%= f.text_field :username, autofocus: true, placeholder: "логин"%>
		<%= f.password_field :password, autocomplete: "off",placeholder:"пароль" %>
		<div class="">
			<%= f.check_box :remember_me %>
			<%= f.label "Запомнить Меня" %>
		</div>
		<%= f.submit "Вход" %>
		<% end %>
	</div>
</div>