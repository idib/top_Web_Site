<div class="log-in" >
	<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
		<%= f.text_field :username, autofocus: true, placeholder: "Логин"%><br/>
		<%= f.password_field :password, autocomplete: "off",placeholder:"Пароль" %><br/>
		<%= f.check_box :remember_me %>
		<%= f.label "Запомнить Меня" %><br/>
		<%= f.submit "Вход" %>
	<% end %>
	<div class="grabber">
		Вход
	</div>
</div>
